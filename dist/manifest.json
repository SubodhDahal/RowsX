{
  "name": "Rows X",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "rows.png",
    "default_popup": "index.html"
  }
}