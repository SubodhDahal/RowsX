{
  "name": "RowsX",
  "version": "1.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": "48x48.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  }
}
